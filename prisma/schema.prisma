generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model suratMasuk {
  id                 Int      @id @default(autoincrement())
  nomorSurat         String
  pengirimSurat      String
  waktuSurat         DateTime
  lampiranSurat      String
  perihalSurat       String
  penerimaSurat      String
  isiSurat           String
  unitPenerbit       String
  tempatSurat        String
  namaMengesahkan    String
  namaTembusan       String
  tanggalInput       DateTime @default(now())
}
